"[ -I { <geheel_getal>[,...] | SUM | ALL | XALL } ] [ -P { <cpu>[,...] | ALL } ]\n"
"[ -m { <sleutelwoord>[,...] | ALL } ] [ -n { <sleutelwoord>[,...] | ALL } ]\n"
"[ -o [ <bestandsnaam> ] | -f [ <bestandsnaam> ] ]\n"
"[ -j { ID | LABEL | PATH | UUID | ... } ]\n"
"[ -f [ <bestandsnaam> ] | -o [ <bestandsnaam> ] | -[0-9]+ ]\n"
